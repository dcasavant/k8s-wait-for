test_should_print_bonjour_monde() {
  JSONPATH='{range .items[*]}{@.metadata.name}:{range @.status.conditions[*]}{@.type}={@.status};{end}{end}';

  kubectl apply -f wait_for_job_success.yaml

  local TIMEOUT=5
  local cmd="kubectl -n default get pods -lapp=travis-example -o jsonpath="$JSONPATH" 2>&1 | grep -q \"Ready=True\""

  until [ $TIMEOUT -eq 4 ] || cmd;
  do
    sleep 1
    echo "waiting for travis-example deployment to be available"
    kubectl get pods -n default
  done

  assert_equals "A" "A"
}
